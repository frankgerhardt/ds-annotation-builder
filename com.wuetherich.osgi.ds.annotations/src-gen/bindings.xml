<jxb:bindings version="2.0" xmlns:jxb="http://java.sun.com/xml/ns/jaxb"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema">
	<jxb:bindings schemaLocation="scr-1_1_0.xsd" node="/xsd:schema">
		<jxb:bindings node="//xsd:complexType[@name='Tproperty']">
			<jxb:bindings node=".//xsd:attribute[@name='value']">
				<jxb:property name="propertyValue" />
			</jxb:bindings>
			<jxb:bindings node=".//xsd:attribute[@name='type']">
				<jxb:property name="propertyType" />
			</jxb:bindings>
			<jxb:bindings node=".//xsd:attribute[@name='name']">
				<jxb:property name="propertyName" />
			</jxb:bindings>
		</jxb:bindings>
	</jxb:bindings>
</jxb:bindings>